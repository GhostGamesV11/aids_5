from typing import Any, Callable


class BinaryNode:
    value: Any
    left_child : 'BinaryNode'
    right_child : 'BinaryNode'

    def __init__(self, value: Any):
        self.value = value
        self.left_child = None
        self.right_child = None

    def __str__(self):
        wynik = str(self.value)
        return wynik

    def is_leaf(self):
        if (self.right_child is None and self.left_child is None):
            return True
        return False


    def add_lef_child(self, value: Any):
        self.left_child = value


    def add_right_child(self, value: Any):
        self.right_child = value


    # def treverse_in_order(self, visit:Callable[[Any],None]):
    #     if visit == None:
    #         return
    #
    #     if self.left_child:
    #         self.left_child.traverse_post_order()
    #
    #     visit(self)
    #
    #     if self.right_child:
    #         self.right_child.traverse_post_order()
    #
    #
    #
    #
    # def traverse_post_order(self, visit: Callable[[Any], None]):
    #     if visit == None:
    #         return
    #
    #
    #     if self.left_child:
    #         self.left_child.traverse_post_order()
    #
    #     if self.right_child:
    #         self.right_child.traverse_post_order()
    #
    #     visit(self)
    #
    #
    #
    # def traverse_pre_order(self, visit: Callable[[Any], None]):
    #     if visit == None:
    #         return
    #
    #     visit(self)
    #
    #     if self.left_child:
    #         self.left_child.treverse_in_order()
    #
    #     if self.right_child:
    #         self.right_child.treverse_in_order()


    def treverse_in_order(self):
        a = []
        if self.left_child:
            a += self.left_child.treverse_in_order()

        a.append(self.value)

        if self.right_child:
            a += self.right_child.treverse_in_order()

        return a

    def traverse_post_order(self):
        a = []
        if self.left_child:
            a += self.left_child.treverse_in_order()

        if self.right_child:
            a += self.right_child.treverse_in_order()

        a.append(self.value)

        return a

    def traverse_pre_order(self):
        a = []

        a.append(self.value)

        if self.left_child:
            a += self.left_child.treverse_in_order()

        if self.right_child:
            a += self.right_child.treverse_in_order()

        return a



    def print(self):
        print(self.treverse_in_order())




class BinaryTree:
    root: BinaryNode

    def __init__(self, root):
        self.root = root

    def traverse_in_order(self, visit: Callable[[Any], None]):
        self.root.treverse_in_order(visit)

    def traverse_post_order(self, visit: Callable[[Any], None]):
        self.root.treverse_post_order(visit)

    def traverse_pre_order(self, visit: Callable[[Any], None]):
        self.root.treverse_pre_order(visit)

    def show(self):
        pass



def check_level(tree, first_node: BinaryNode, secend_node: BinaryNode)-> BinaryNode:
    def Level(node, value ,level ):
        if (node == None):
            return 0

        if (node.value == value):
            return level

        lower_level = Level(node.left_child, value, level + 1)
        if (lower_level != 0):
            return lower_level

        lower_level = Level(node.right_child, value, level + 1)
        return lower_level

    def getLevel(node, value):
        return Level(node, value, 1)


    a = getLevel(tree.root,first_node.value)
    b = getLevel(tree.root, secend_node.value)

    if (a==b):
        return print("Prawda że:",first_node," i ",secend_node,"są na tym samym poziomie")
    return print("Fałsz że:",first_node," i ",secend_node,"są na tym samym poziomie")




ten = BinaryNode(10)
tree = BinaryTree(ten)
nine = BinaryNode(9)
one = BinaryNode(1)
three = BinaryNode(3)
two = BinaryNode(2)
four = BinaryNode(4)
six = BinaryNode(6)




ten.add_right_child(two)
ten.add_lef_child(nine)
nine.add_lef_child(one)
nine.add_right_child(three)
two.add_lef_child(four)
two.add_right_child(six)


# tree.root.print()

check_level(tree,nine,one)




